    <block type="shape_cube"><title name="CENTEROBJECT">TRUE</title><value name="width"><block type="math_number"><title name="NUM">10</title></block></value><value name="depth"><block type="math_number"><title name="NUM">10</title></block></value><value name="height"><block type="math_number"><title name="NUM">10</title></block></value></block>
